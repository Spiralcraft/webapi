<rpc:Filter
  xmlns:rpc="class:/spiralcraft/servlet/rpc/"
  xmlns:af="class:/spiralcraft/servlet/autofilter/"
  xmlns:snm="class:/spiralcraft/net/mime/"
  >
  <reflectOriginHeader>true
  </reflectOriginHeader>
  
  <headers>
    <snm:MimeHeader>Access-Control-Allow-Credentials: true
    </snm:MimeHeader>
    <snm:MimeHeader>Access-Control-Allow-Headers: Content-Type, *
    </snm:MimeHeader>
  </headers>

  
  <handlers>
    <HelloHandler/>
    <LoginHandler/>
  </handlers>
</rpc:Filter>
